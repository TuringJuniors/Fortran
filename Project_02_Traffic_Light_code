

#define RED 13
#define GREEN 10
#define YELLOW 12
#define YELLOW2 11

void setup()
{
  pinMode(RED, OUTPUT);
  pinMode(YELLOW, OUTPUT);
  pinMode(GREEN, OUTPUT);
  pinMode(YELLOW2, OUTPUT);
}


 void loop()
{
   int LED=RED;YELLOW;YELLOW2;GREEN; 
   
 switch (LED)
 {
  case RED:      
  digitalWrite(13, HIGH);   // turn the RED LED on (HIGH is the voltage level)
  delay(3000); // Wait for 3000 millisecond(s)
 
  digitalWrite(13, LOW);   // turn the RED LED off by making the voltage LOW
  delay(1000); // Wait for 1000 millisecond(s);
   
   
  case YELLOW:
  digitalWrite(12, HIGH);   // turn the YELLOW LED on (HIGH is the voltage level)
  delay(1500); // Wait for 1000 millisecond(s)
 
  digitalWrite(12, LOW);    // turn the YELLOW LED off by making the voltage LOW
  delay(1500); // Wait for 1000 millisecond(s);
   
  
  case GREEN:
  digitalWrite(10, HIGH);   // turn the GREEN LED on (HIGH is the voltage level)
  delay(1500); // Wait for 1000 millisecond(s)
      
  digitalWrite(10, LOW);    // turn the Green LED off by making the voltage LOW
  delay(1500); // Wait for 1000 millisecond(s);
   
   
 
    
  
  default:
  case YELLOW2:
  digitalWrite(11, HIGH);   // turn the YELLOW2 LED on (HIGH is the voltage level)
  delay(1500); // Wait for 1000 millisecond(s)
 
  digitalWrite(11, LOW);    // turn the YELLOW2 LED off by making the voltage LOW
  delay(1500); // Wait for 1000 millisecond(s);
  
  } 
    
}
   
  

  

